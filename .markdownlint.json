{
    "code-block-style": { "style": "fenced" },
    "code-fence-style": { "style": "backtick"},
    "first-line-h1": false,
    "heading-style": { "style": "atx" },
    "line-length": {
        "code_blocks": false,
        "tables": false
    },
    "list-marker-space": {
        "ul_single": 3,
        "ul_multi": 3,
        "ol_single": 2,
        "ol_multi": 2
    },
    "no-inline-html": false,
    "no-trailing-punctuation": { "punctuation": "." },
    "ol-prefix": { "style": "one" },
    "ul-indent": { "indent": 4 },
    "ul-style": { "style": "asterisk" }
}
